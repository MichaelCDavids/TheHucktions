{{> header.handlebars}}
<main role="main">
    <div id="myCarousel" class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img class="myImage" src="/images/carousel-3.jpg">
                <div class="container">
                    <div class="carousel-caption text-left">
                        <h1 class="text-light">Fixtures List</h1>
                        <p class="text-light">Plan strategies view next opponents </p>
                        <p><a class="btn btn-lg btn-primary" href="/fixtures" role="button">Next Fixtures</a></p>
                    </div>
                </div>
            </div>
            <div class="carousel-item">
                <img class="myImage img-fluid" src="/images/team.jpg">
                <div class="container">
                    <div class="carousel-caption">
                        <h1 class="text-dark">Team Selection</h1>
                        <p class="text-dark">Select Your team for next match</p>
                        <p><a class="btn btn-lg btn-primary" href="/players" role="button">Squad</a></p>
                    </div>
                </div>
            </div>
            <div class="carousel-item">
                <img class="myImage img-fluid" src="/images/carousel-3.jpg">
                <div class="container">
                    <div class="carousel-caption text-right">
                        <h1>Squad Pictures</h1>
                        <p><a class="btn btn-lg btn-primary" href="/gallery" role="button">View Gallery</a></p>
                    </div>
                </div>
            </div>
        </div>
        <a class="carousel-control-prev" href="#myCarousel" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#myCarousel" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>

